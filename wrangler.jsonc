/**
 * For more details on how to configure Wrangler, refer to:
 * https://developers.cloudflare.com/workers/wrangler/configuration/
 */
{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "auth",
	"main": ".open-next/worker.js",
	"compatibility_date": "2025-10-08",
	"compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
	"assets": {
		"binding": "ASSETS",
		"directory": ".open-next/assets",
	},
	"observability": {
		"enabled": true,
	},
	"upload_source_maps": true,
	// Each deployment should have its own wrangler config with the appropriate AUTH_CORE_BASE_URL
	// For dev/preview: "auth-svc.janovix.workers.dev"
	// For production: "auth-svc.janovix.ai"
	"vars": {
		"AUTH_CORE_BASE_URL": "auth-svc.janovix.workers.dev",
		"NEXT_PUBLIC_AUTH_CORE_BASE_URL": "auth-svc.janovix.workers.dev",
	},
}
